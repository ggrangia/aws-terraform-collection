{
  "executionRoleArn": "arn:aws:iam::089722027487:role/client_task_role",
  "requiresCompatibilities": ["FARGATE"],
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "family": "client_task",
  "containerDefinitions": [
    {
      "name": "client",
      "image": "089722027487.dkr.ecr.us-east-1.amazonaws.com/my_ecr:latest",
      "essential": true,
      "cpu": 0,
      "portMappings": [
        {
          "containerPort": 9090,
          "hostPort": 9090,
          "protocol": "tcp"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-group": "/aws/ecs/client_lg",
          "awslogs-region": "us-east-1",
          "awslogs-stream-prefix": "ecs"
        }
      },
      "environment": []
    }
  ]
}
